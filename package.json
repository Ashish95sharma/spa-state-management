{
  "name": "dynamic-mfe-state",
  "version": "0.0.2",
  "description": "Dynamic, reducer-injection state manager for microfrontends (framework-agnostic).",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "microfrontend",
    "state",
    "reducer",
    "dynamic",
    "redux-like",
    "store",
    "module-federation"
  ],
  "author": "",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "@babel/runtime": "^7.23.3"
  }
}

